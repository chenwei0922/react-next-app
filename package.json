    "@reown/appkit-adapter-wagmi": "^1.8.15",
    "@tanstack/react-query": "^5.90.12",
    "ahooks": "^3.9.6",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "fabric": "^6.9.0",
    "framer-motion": "^12.23.24",
